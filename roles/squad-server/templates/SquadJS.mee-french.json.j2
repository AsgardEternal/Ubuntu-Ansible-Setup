    "rconPassword": "{{ server.secrets.rcon_password }}",
    "logReaderMode": "tail",
    "logDir": "/home/squadjs/SquadLogs",
    "ftp": {
        "port": 21,
        "user": "FTP Username",
        "password": "FTP Password",
        "useListForSize": false
